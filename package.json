{
  "name": "ns-nepali-date",
  "title": "NS Nepali Date",
  "version": "1.0.10",
  "license": "GPL-2.0-or-later",
  "main_file": "ns-nepali-date.php",
  "repository": {
    "type": "git",
    "url": "https://github.com/ernilambar/ns-nepali-date.git"
  },
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "prod": "gulp build --env production",
    "eslint": "eslint --quiet .",
    "eslint:fix": "eslint --quiet --fix .",
    "format": "prettier --write \"src/**/*.scss\"",
    "preinstall": "npx only-allow pnpm",
    "version": "easy-replace-in-files",
    "pot": "wpi18n makepot --domain-path=languages --exclude=vendor,deploy,node_modules",
    "textdomain": "wpi18n addtextdomain --exclude=vendor,deploy,node_modules",
    "predeploy": "shx rm -rf vendor/ && composer install --no-dev --no-scripts -o && shx rm -rf assets/ && pnpm run prod",
    "deploy": "shx rm -rf deploy/ && shx mkdir deploy && copy-files-from-to && cd deploy/ && cross-var shx mv temp $npm_package_name && cross-var bestzip ../$npm_package_name.zip * && cd .. && cross-var shx mv $npm_package_name.zip deploy/"
  },
  "browserslist": "> 1%, not dead",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@wordpress/eslint-plugin": "^12.8.0",
    "@wordpress/prettier-config": "^1.4.0",
    "bestzip": "^2.2.1",
    "browser-sync": "^2.27.10",
    "copy-files-from-to": "^3.2.2",
    "cross-var": "^1.1.0",
    "dotenv": "^16.0.2",
    "easy-replace-in-files": "^1.0.3",
    "eslint": "^8.21.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-environments": "^1.0.1",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "node-wp-i18n": "^1.2.6",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "shx": "^0.3.4"
  },
  "prettier": "@wordpress/prettier-config"
}
